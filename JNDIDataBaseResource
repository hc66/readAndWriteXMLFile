public class JNDIDataBaseResource {
	private String auth;
	private String driverClassName;
	private String initialSize;
	private String maxIdle;
	private String maxTotal;
	private String maxWaitMillis;
	private String name;
	private String password;
	private String type;
	private String url;
	private String username;
	public String getAuth() {
		return auth;
	}
	public JNDIDataBaseResource setAuth(String auth) {
		this.auth = auth;
		return this;
	}
	public String getDriverClassName() {
		return driverClassName;
	}
	public JNDIDataBaseResource setDriverClassName(String driverClassName) {
		this.driverClassName = driverClassName;
		return this;
	}
	public String getInitialSize() {
		return initialSize;
	}
	public JNDIDataBaseResource setInitialSize(String initialSize) {
		this.initialSize = initialSize;
		return this;
	}
	public String getMaxIdle() {
		return maxIdle;
	}
	public JNDIDataBaseResource setMaxIdle(String maxIdle) {
		this.maxIdle = maxIdle;
		return this;
	}
	public String getMaxTotal() {
		return maxTotal;
	}
	public JNDIDataBaseResource setMaxTotal(String maxTotal) {
		this.maxTotal = maxTotal;
		return this;
	}
	public String getMaxWaitMillis() {
		return maxWaitMillis;
	}
	public JNDIDataBaseResource setMaxWaitMillis(String maxWaitMillis) {
		this.maxWaitMillis = maxWaitMillis;
		return this;
	}
	public String getName() {
		return name;
	}
	public JNDIDataBaseResource setName(String name) {
		this.name = name;
		return this;
	}
	public String getPassword() {
		return password;
	}
	public JNDIDataBaseResource setPassword(String password) {
		this.password = password;
		return this;
	}
	public String getType() {
		return type;
	}
	public JNDIDataBaseResource setType(String type) {
		this.type = type;
		return this;
	}
	public String getUrl() {
		return url;
	}
	public JNDIDataBaseResource setUrl(String url) {
		this.url = url;
		return this;
	}
	public String getUsername() {
		return username;
	}
	public JNDIDataBaseResource setUsername(String username) {
		this.username = username;
		return this;
	}

	@Override
	public String toString() {
		return "{" +
					"\"auth\":" + "\"" + auth + "\"," +
					"\"driverClassName\":" + "\"" + driverClassName + "\"," +
					"\"initialSize\":" + "\"" + initialSize + "\"," +
					"\"maxIdle\":" + "\"" + maxIdle + "\"," +
					"\"maxTotal\":" + "\"" + maxTotal + "\"," +
					"\"maxWaitMillis\":" + "\"" + maxWaitMillis + "\"," +
					"\"name\":" + "\"" + name + "\"," +
					"\"password\":" + "\"" + password + "\"," +
					"\"type\":" + "\"" + type + "\"," +
					"\"url\":" + "\"" + url + "\"," +
					"\"username\":" + "\"" + username + "\"" +
				"}";
	}
}
